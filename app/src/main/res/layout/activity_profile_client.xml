<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.capstone.mobile.coachwithoutsearch.ProfileActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="329dp"
        android:layout_height="162dp"
        android:background="@drawable/customer_profile_details"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="43dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.526" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="53dp"
        android:text="@string/basicinfo"
        android:textColor="@color/iconColor"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Name:"
        android:textColor="@color/iconColor"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Contact Number:"
        android:textColor="@color/iconColor"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="Email:"
        android:textColor="@color/iconColor"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="First Name:"
        android:textColor="@color/iconColor"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="65dp"
        android:layout_marginTop="53dp"
        android:text="EDIT"
        android:textColor="@color/iconColor"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/editButton"
        android:layout_width="13dp"
        android:layout_height="17dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="53dp"
        app:layout_constraintLeft_toRightOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gear"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="@+id/textView5" />

    <TextView
        android:id="@+id/stfFName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/iconColor"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="@+id/textView5"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/stfLName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:text="TextView"
        android:textColor="@color/iconColor"
        app:layout_constraintRight_toRightOf="@+id/textView5"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/stfFName" />

    <TextView
        android:id="@+id/stfContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/iconColor"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/stfLName"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="@+id/textView5" />

    <TextView
        android:id="@+id/stfEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/iconColor"
        app:layout_constraintRight_toRightOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/stfContact" />
</android.support.constraint.ConstraintLayout>
